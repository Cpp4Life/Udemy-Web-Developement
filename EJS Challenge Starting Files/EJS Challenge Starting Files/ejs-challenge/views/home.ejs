<%- include('partials/header'); -%>
    <h1>Home</h1>
    <p>
        <%= startingContent %>
    </p>
    <% homeContent.forEach(element=> { %>
        <h1>
            <%= element.title %>
        </h1>
        <p>
            <%= element.content.substring(0, 100) + '... ' %><a href='/posts/<%= element._id %>'>Read more</a>
        </p>

        <% }) %>
            <a class="btn btn-primary" href="/compose" role="button">Compose</a>
            <%- include('partials/footer'); -%>